		        <div class="box-name">
		            <i class="fa fa-search"></i><span>@ResourceKey("Settings.countrydetails")</span>
		        </div>
		        <div class="box-icons">
		            <a class="collapse-link"><i class="fa fa-chevron-down"></i>
		            </a><a class="expand-link"><i class="fa fa-expand"></i></a>
		            <a class="close-link"><i class="fa fa-times"></i></a></div>
		        <div class="no-move">
		        </div>
		    </div>
		
		    <div class="box-content">
		        <div class="form-horizontal dnnForm">
		            <div class="form-group">
		                <label class="col-sm-3 control-label">@ResourceKey("Settings.merchantculture")</label>
		                <div class="col-sm-5">[<tag id="merchantculturecode" cssclass="s2_with_tag populate placeholder" type="culturecodedropdown"/>]
		                </div>
		            </div>
		            <div class="form-group">
		                <label class="col-sm-3 control-label">@ResourceKey("Settings.currencycode")</label>
		                <div class="col-sm-5">[<tag id="currencyculturecode" cssclass="s2_with_tag populate placeholder" type="culturecodedropdown"/>]
		                </div>
		            </div>
		            <div class="form-group">
		                <label class="col-sm-3 control-label">@ResourceKey("Settings.currencysymbol")</label>
		                <div class="col-sm-2">
		                    [<tag id="currencysymbol" cssclass="form-control" type="textbox" maxlength="4" Text="$"/>]
		                </div>
		            </div>
		        </div>
		        <div class="form-horizontal dnnForm countrylist">
		            <div class="form-group">
		                <div class="col-sm-3 eurocountries">
		                    <p class="page-header">@ResourceKey("Settings.commoncountries")</p>
		                </div>
		                <div class="col-sm-6">
		                    <p class="page-header">@ResourceKey("Settings.selectedcountries")</p>[<tag type="countrylist" id="countrylist"/>]
		                </div>
		            </div>

		        </div>
		        <div class="form-horizontal dnnForm countryselection">
		            <p class="page-header">@ResourceKey("Settings.othercountries") <input type="checkbox" class="checkAll" /> @ResourceKey("Settings.checkall")</p>
		            [<tag id="countrycodelist" type="dnn:countrycheckboxlist" RepeatColumns="4"/>]
		        </div>
		    </div>
		</div>
	</div>
</div>


